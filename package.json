{
  "name": "miyajima-tide",
  "version": "1.0.0",
  "description": "JavaScript library for calculating tidal height at Miyajima (Itsukushima) using harmonic analysis based on textbook equations",
  "type": "module",
  "main": "tide-lib.js",
  "exports": {
    ".": {
      "import": "./tide-lib.js"
    }
  },
  "scripts": {
    "serve": "node ./scripts/serve.mjs --port 8200 --host 0.0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsuchim/miyajima-tide.git"
  },
  "keywords": [
    "tide",
    "tidal",
    "miyajima",
    "itsukushima",
    "harmonic-analysis",
    "ocean",
    "javascript",
    "es-module"
  ],
  "author": "tsuchim",
